@model Queue.ViewModels.UserRegister

@{
    ViewBag.Title = "User Delete";

}
<div class="text-left">
    <h1 class="display-4">Delete Your Information</h1>
</div>

<dl class="row">
    <dt class="col-sm-2">@Html.DisplayNameFor(model => model.UserFirstName)</dt>
    <dd class="col-sm-10">@Html.DisplayFor(model => model.UserFirstName)</dd>
    <dt class="col-sm-2">@Html.DisplayNameFor(model => model.UserLastName)</dt>
    <dd class="col-sm-10">@Html.DisplayFor(model => model.UserLastName)</dd>
    <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Email)</dt>
    <dd class="col-sm-10">@Html.DisplayFor(model => model.Email)</dd>
    <dt class="col-sm-2">@Html.DisplayNameFor(model => model.PhoneNumber)</dt>
    <dd class="col-sm-10">@Html.DisplayFor(model => model.PhoneNumber)</dd>
</dl>
<h6>Are you sure?!</h6>
<form method="post" asp-action="delete">
    <button type="submit" class="btn btn-danger">Delete</button>
</form>